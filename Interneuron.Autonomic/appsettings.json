{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Settings": {
  
    "AuthorizationAuthority": "YOUR_SYNAPSE_IDENTITY_SERVER_URL",
 
    "AuthorizationAudience": "dynamicapi",
